<core:FragmentDefinition
	xmlns="sap.suite.ui.commons"
	xmlns:m="sap.m"
	xmlns:layout="sap.ui.layout"
	xmlns:core="sap.ui.core">
	<layout:Splitter width="100%" height="100%">
		<layout:contentAreas>
			<ChartContainer
				id="chartContainer"
				showFullScreen="true"
				showPersonalization="false"
				autoAdjustHeight="false"
				personalizationPress="attachPersonalizationPress"
				contentChange="attachContentChange"
				title="Revenue">
				<dimensionSelectors>
					<m:Select id="idoSelect1" change="handleSelectionChange">
                        <core:Item key="2" text="Abuse"/>
                        <core:Item key="3" text="Crime"/>
                        <core:Item key="4" text="Education"/>
                        <core:Item key="5" text="Employment"/>
                        <core:Item key="6" text="Health"/>
                        <core:Item key="7" text="Social Media"/>
					</m:Select>
				</dimensionSelectors>
			</ChartContainer>
		</layout:contentAreas>
	</layout:Splitter>
</core:FragmentDefinition>